{
  "name": "html-language-features",
  "version": "1.0.0",
  "description": "HTML Language Features",
  "publisher": "SimonSiefke",
  "main": "dist/extensionMain.js",
  "enableProposedApi": true,
  "activationEvents": [
    "onLanguage:html"
  ],
  "engines": {
    "vscode": "^1.35.0"
  },
  "scripts": {
    "dev": "cd ../../ && npm run dev",
    "update-proposed": "vscode-dts dev"
  },
  "contributes": {
    "keybindings": {
      "command": "html-expand-abbreviation",
      "key": "tab",
      "when": "editorTextFocus && resourceLangId == html && !editorReadonly"
    },
    "commands": [
      {
        "command": "html-expand-abbreviation",
        "title": "expand abbreviation"
      }
    ]
  },
  "keywords": [
    "HTML",
    "HTML IntelliCode",
    "HTML Language Features"
  ],
  "author": "Simon Siefke",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/vscode": "^1.35.0",
    "@types/ws": "^6.0.1",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "vscode-test": "^1.0.0",
    "ws": "^7.0.1"
  },
  "dependencies": {
    "vscode-languageclient": "^5.3.0-next.6"
  }
}
