{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/files*/**": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "json.schemas": [
    {
      "fileMatch": ["**/*.htmlData2.json"],
      "url": "./packages/schema/src/schema/schema.json"
    },
    {
      "fileMatch": ["**/*.htmlData.json"],
      "url": "./packages/schema/src/schema/schema.json"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.vscode": true,
    "**/tsconfig.json": true,
    "**/tsconfig*.json": true,
    "**/*.webpack.config.js": true,
    ".travis.yml": true,
    ".gitignore": true,
    "fixtures": true,
    "scripts": true,
    "**/*.d.ts": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/deprecated": true
  }
}
