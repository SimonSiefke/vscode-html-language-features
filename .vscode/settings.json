{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/files*/**": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "fileutils.typeahead.exclude": {
    "**/dist": true
  },
  "json.schemas": [
    {
      "fileMatch": ["**/*.htmlData.json"],
      "url": "./packages/schema/src/schema/schema.json"
    }
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
